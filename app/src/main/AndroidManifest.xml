<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.wednesday">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:largeHeap="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activities.LogInActivity"
            android:label="@string/app_name"
            android:theme="@style/noTitle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.SignUpActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activities.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activities.EventlistActivity" />
        <activity android:name=".activities.EventDetailsActivity" />
        <activity android:name=".activities.ActivityDetailsActivity" />
        <activity android:name=".activities.ActivitylistActivity" />
        <activity android:name=".activities.DineoutDetailsActivity" />
        <activity android:name=".activities.DineoutlistActivity" />
        <activity
            android:name=".activities.DineoutFilter"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.ActivitiesFilter"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.EventFilter"
            android:parentActivityName=".activities.MainActivity" />
        <activity android:name=".activities.AddItemActivity" />
        <activity android:name=".activities.AddEvent" />
        <activity android:name=".activities.AddActivity" />
        <activity android:name=".activities.AddDineout"></activity>
    </application>

</manifest>